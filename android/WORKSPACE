    name = "androidsdk",
    # Replace with your installed Android SDK API level
    api_level = 25
)

bind(
 name = "support-appcompat",
 actual = "@androidsdk//com.android.support:appcompat-v7-25.1.1",
)

bind(
 name = "support-annotations",
 actual = "@androidsdk//com.android.support:support-annotations-25.1.1",
)